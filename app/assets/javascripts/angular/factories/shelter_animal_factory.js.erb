app.factory('ShelterAnimalFactory', ShelterAnimalFactory)

  ShelterAnimalFactory.$inject = ['$resource'];

  var url = "<%= ENV['URL'] %>" || "http://localhost:3000";

  function ShelterAnimalFactory($resource) {
    return $resource( url + '/api/shelters/:shelterId/animals/:id',
      {shelterId: '@shelterId', id: '@id'},
      {'update': { method:'PUT' }}
    );
  }

