app.factory('ShelterFactory', ShelterFactory)

  ShelterFactory.$inject = ['$resource'];

  var url = "<%= ENV['URL'] %>" || "http://localhost:3000";

  function ShelterFactory($resource) {
   return $resource(url + '/api/shelters/:id',
      {id: '@_id'},
      {'update': { method:'PUT' }}
    );
  }

