app.controller('AdminShelterCtrl', ['$scope', '$auth', '$http', '$routeParams', 'ShelterFactory', 'ShelterAnimalFactory' '$stateParams', function ($scope, $auth, $http, $routeParams, ShelterFactory, ShelterAnimalFactory, $stateParams){
  console.log("AdminShelterCtrl");
